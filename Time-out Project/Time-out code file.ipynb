   "source": [
    "import time\n",
    "\n",
    "my_time= int(input(\"Enter the time \"))\n",
    "for x in reversed(range(0,my_time)):\n",
    "    seconds= x % 60\n",
    "    minutes = int(x/60)% 60\n",
    "    print(f\"{minutes:02}:{seconds:02}\",end=\"\\r\")\n",
    "    time.sleep(1)\n",
    "\n",
    "print(\"Time's Up\")"
   ]
  }
